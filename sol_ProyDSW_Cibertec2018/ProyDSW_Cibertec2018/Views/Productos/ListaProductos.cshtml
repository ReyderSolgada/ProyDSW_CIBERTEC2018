@model IEnumerable<Dominio.Core.Entidades.Productos>

@{
    ViewBag.Title = "ListaProductos";
}

<h2>ListaProductos</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.codpro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nompro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.stock)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.precio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.codtipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eliminado)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.codpro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nompro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.stock)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.precio)
            </td>
            
        </tr>
    }

</table>

